{
  "name": "@kikilo/kikilo-uilib-components",
  "version": "1.1.1",
  "description": "Set of react componentes for kikilo project",
  "private": false,
  "scripts": {
    "build": "sui-studio build",
    "check:release": "sui-studio check-release",
    "co": "sui-studio commit",
    "commitmsg": "validate-commit-msg",
    "dev": "sui-studio dev",
    "generate": "sui-studio generate --prefix kikilo --scope kikilo-uilib-components",
    "lint:js": "sui-lint js",
    "lint:sass": "sui-lint sass",
    "lint": "npm run lint:js && npm run lint:sass",
    "phoenix:ci": "npx @s-ui/mono phoenix --ci ",
    "phoenix": "npx @s-ui/mono phoenix",
    "precommit": "sui-precommit run",
    "release": "sui-studio release",
    "start": "sui-studio start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscar-raig/kikilo-uilib-components.git"
  },
  "keywords": [],
  "author": "oscar.raig@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@s-ui/precommit": "2",
    "@s-ui/studio": "7",
    "husky": "0.14.3",
    "validate-commit-msg": "2.14.0"
  },
  "dependencies": {},
  "config": {
    "sui-mono": {
      "access": "public",
      "packagesFolder": "./components",
      "deepLevel": 2
    },
    "validate-commit-msg": {
      "types": "@s-ui/mono/src/types"
    }
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "stylelint": {
    "extends": "./node_modules/@s-ui/lint/stylelint.config.js"
  },
  "prettier": "./node_modules/@s-ui/lint/.prettierrc.js"
}
